{
  "name": "components",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn prestorybook && react-native --port 9082 run-android",
    "ios": "yarn prestorybook && react-native --port 9081 run-ios --udid 00008101-000A512A1A06001E",
    "web": "yarn sb:web",
    "sb:web": "yarn prestorybook && ../../node_modules/@storybook/react/bin/index.js -p 7008",
    "sb:mob": "yarn prestorybook && start-storybook -p 7007 -h 192.168.0.14",
    "ios-bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "test": "jest",
    "lint": "eslint .",
    "prestorybook": "rnstl",
    "build": "../../node_modules/@storybook/react/bin/build.js",
    "xcode": "xed ios",
    "studio": "studio android",
    "dist": "npm run clean && npm run copy-files && npm run dist:babel && npm run dist:types && node ./scripts/copyTS.js",
    "dist:babel": "npm run dist:babel:cjs",
    "dist:babel:cjs": "BABEL_ENV=cjs babel src --source-maps --extensions \".js,.ts,.tsx\" --ignore \"src/**/*.stories.tsx,src/**/*.stories.ts\" --out-dir dist --presets=@babel/env",
    "dist:types": "tsc -p tsconfig.gen-dts.json",
    "clean": "rimraf dist",
    "copy-files": "copyfiles -u 1 \"src/**/*.scss\" \"src/**/*.css\" dist",
    "cp-app": "cp '../../app.json' ./app.json",
    "rn-clean": "npx react-native-clean-project --keep-node-modules --remove-iOS-build --keep-system-iOS-pods-cache false --keep-user-iOS-pods-cache false --keep-pods false --remove-iOS-pods --remove-android-build --clean-android-project --keep-brew",
    "rename": "yarn rn-clean && cd android && npx react-native-rename $(node -pe 'require(\"../../../app.json\").name') && cd ../ios && pod install && cd ../ && yarn cp-app"
  },
  "dependencies": {
    "fbemitter": "3.0.0",
    "native-base": "3.4.6",
    "navigation": "6.1.0",
    "navigation-react": "4.5.0",
    "navigation-react-native": "8.9.0",
    "navigation-react-native-web": "1.1.0",
    "react-native-safe-area-context": "4.2.5",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "12.3.0",
    "sass-loader": "10.2.1",
    "styled-components": "5.3.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-private-methods": "7.17.12",
    "@babel/plugin-proposal-private-property-in-object": "7.17.12",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime": "7.8.4",
    "@emotion/core": "10.0.27",
    "@mdx-js/loader": "1.6.22",
    "@react-native-async-storage/async-storage": "1.17.3",
    "@react-native-community/eslint-config": "2.0.0",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.25",
    "@storybook/addon-react-native-web": "0.0.18",
    "@storybook/addons": "6.5.9",
    "@storybook/api": "6.5.9",
    "@storybook/components": "6.5.9",
    "@storybook/core-events": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@storybook/theming": "6.5.9",
    "@types/fbemitter": "2.0.32",
    "@types/react": "17.0.0",
    "@types/react-native": "0.67.8",
    "@types/styled-components": "5.1.25",
    "@types/styled-components-react-native": "5.1.3",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "4.0.0",
    "babel-eslint": "10.0.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-react-native-web": "0.17.7",
    "babel-plugin-styled-components": "2.0.7",
    "concurrently": "5.3.0",
    "copyfiles": "2.4.1",
    "core-js": "3.6.5",
    "css-loader": "5.2.7",
    "eslint": "7.5.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-react-native": "3.8.1",
    "eslint-plugin-storybook": "0.5.12",
    "html-webpack-plugin": "4.3.0",
    "husky": "7.0.4",
    "jest": "25.1.0",
    "lint-staged": "12.3.7",
    "metro-react-native-babel-preset": "0.67.0",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.30.0",
    "react-is": "17.0.2",
    "react-markdown": "8.0.3",
    "react-native": "0.68.0",
    "react-native-safe-area-context": "4.2.5",
    "react-native-sass-transformer": "2.0.0",
    "react-native-storybook-loader": "2.0.5",
    "react-native-typescript-transformer": "1.2.13",
    "react-native-vector-icons": "9.1.0",
    "react-native-web": "0.17.7",
    "react-test-renderer": "17.0.2",
    "regenerator-runtime": "*",
    "sass": "1.51.0",
    "storybook-dark-mode": "1.1.0",
    "style-loader": "2.0.0",
    "styled-components": "5.3.5",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,html,md,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.@(js|jsx|ts|tsx)",
      "outputFile": "./storybook/storyLoader.ts"
    }
  }
}
