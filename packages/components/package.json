{
  "name": "components",
  "version": "0.0.1",
  "scripts": {
    "ios": "yarn run prestorybook && react-native --port 8081 run-ios",
    "android": "yarn run prestorybook && react-native --port 8082 run-android",
    "start:web": "yarn run prestorybook && ./node_modules/@storybook/react/bin/index.js -p 8083",
    "start:mob": "yarn run prestorybook && start-storybook -p 7007",
    "ios-bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "test": "jest",
    "lint": "eslint .",
    "prestorybook": "rnstl",
    "build": "./node_modules/@storybook/react/bin/build.js",
    "xcode": "xed ios",
    "studio": "studio android",
    "dist": "npm run clean && npm run copy-files && npm run dist:babel && npm run dist:types && node ./scripts/copyTS.js",
    "dist:babel": "npm run dist:babel:cjs",
    "dist:babel:cjs": "BABEL_ENV=cjs babel src --source-maps --extensions \".js,.ts,.tsx\" --ignore \"src/**/*.stories.tsx,src/**/*.stories.ts\" --out-dir dist --presets=@babel/env",
    "dist:types": "tsc -p tsconfig.gen-dts.json",
    "clean": "rimraf dist",
    "copy-files": "copyfiles -u 1 \"src/**/*.scss\" \"src/**/*.css\" dist"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "outputFile": "./storybook/storyLoader.ts"
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "styled-components": ">=5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.8.4",
    "@mdx-js/loader": "1.6.22",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-docs": "5.3.21",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "5.3.21",
    "@storybook/addon-ondevice-actions": "5.3.21",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/addons": "5.3.21",
    "@storybook/react": "5.3.21",
    "@storybook/react-native": "5.3.21",
    "@storybook/react-native-server": "5.3.21",
    "@types/fbemitter": "^2.0.32",
    "@types/react": "^18.0.1",
    "@types/react-native": "^0.67.4",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-native-web": "^0.17.7",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.7",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-native": "^3.8.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^25.1.0",
    "lint-staged": "^12.3.7",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.6.2",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^7.30.0",
    "react-markdown": "^8.0.3",
    "react-native": "0.68.0",
    "react-native-sass-transformer": "^2.0.0",
    "react-native-storybook-loader": "^2.0.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.7",
    "react-test-renderer": "16.13.1",
    "sass": "^1.51.0",
    "style-loader": "^2.0.0",
    "styled-components": "5.3.5",
    "typescript": "^4.6.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,html,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "elsewhere-shared-types": "https://gitlab.com/elsewhereapp/elsewhere-shared-types",
    "fbemitter": "^3.0.0",
    "native-base": "^3.4.3",
    "navigation": "^6.1.0",
    "navigation-react": "^4.4.0",
    "navigation-react-native": "^8.8.2",
    "navigation-react-native-web": "^1.0.1",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-svg": "^12.3.0",
    "sass-loader": "^10.2.1"
  }
}
